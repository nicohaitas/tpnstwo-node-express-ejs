<section id="happening-now">
    <main>
        <div class="content-area-lead-story">
            <%_ for (var i = 0 ; i < data.articles.length; i++) { _%>
                <%_ eachArticle = data.articles[i]; _%>
                <%_ var thisArticle = eachArticle.leadarticle; _%>
                <%_ if ( thisArticle === 1 ) { _%>
                    <article>
                        <div class="article-category-main-container">
                            <div class="article-category-container">
                                <a>
                                    <h1 class="article-headline"><%- eachArticle.headline %></h1>
                                    <%_ if ( eachArticle.kickerimage350 ) { _%>
                                    <div class="article-image <%- eachArticle.imageorientation %>">
                                        <div class="new-article-image compat-object-fit" style="background-image: url('<%- eachArticle.kickerimage350 %>');">
                                            <img src="<%- eachArticle.kickerimage350 %>" alt="Image for <%- eachArticle.kicker %> article" />
                                        </div>
                                    </div>
                                    <h3 class="article-subhead"><%- eachArticle.subhead %></h3>
                                    <%_ } else { _%>
                                    <h3 class="article-subhead"><%- eachArticle.subhead %></h3>
                                    <div class="article-nutshell"><%- eachArticle.nutshell %></div>
                                    <%_ } _%>
                                </a>
                            </div>
                            <div class="article-additional-info">
                                <small class="article-category-identifier-container">
                                    Featured in: 
                                    <%_ if ( eachArticle.categoryslug ) { _%>
                                    <a href="<%- eachArticle.categoryslug %>"><span class="category-identifier"><%- eachArticle.categoryslug %></span></a>
                                    <%_ } _%>
                                    <%_ if ( eachArticle.subcategoryslug ) { _%>
                                        - <a href="<%- eachArticle.categoryslug %>/<%- eachArticle.subcategoryslug %>"><span class="subcategory-identifier"><%- eachArticle.subcategoryslug %></span></a>
                                    <%_ } _%>
                                    <%_ if ( eachArticle.subcategoryslug && eachArticle.subcategoryfirstchildslug ) { _%>
                                        - <a href="<%- eachArticle.categoryslug %>/<%- eachArticle.subcategoryslug %>/<%- eachArticle.subcategoryfirstchildslug %>"><span class="subcategory-first-child-identifier"><%- eachArticle.subcategoryfirstchildslug %></span></a>
                                    <%_ } _%>
                                    <%_ if ( eachArticle.subcategoryslug && eachArticle.subcategoryfirstchildslug && eachArticle.subcategorysecondchildslug ) { _%>
                                        - <a href="<%- eachArticle.categoryslug %>/<%- eachArticle.subcategoryslug %>/<%- eachArticle.subcategoryfirstchildslug %>/<%- eachArticle.subcategorysecondchildslug %>"><span class="subcategory-second-child-identifier"><%- eachArticle.subcategorysecondchildslug %></span></a>
                                    <%_ } _%>
                                </small>
                                <small class="article-authors-container">
                                    By: 
                                    <%_ if ( eachArticle.reporter1 ) { _%>
                                        <a href=""><span class="article-author"><%- eachArticle.reporter1 %></span></a>
                                    <%_ } _%>
                                    <%_ if ( eachArticle.reporter1 && eachArticle.reporter2 ) { _%>
                                        and <a href=""><span class="article-author"><%- eachArticle.reporter2 %></span></a>
                                    <%_ } _%>
                                </small>
                            </div>
                            <div class="article-controller-container">
                                <div class="article-controller-pin-article">
                                    <a title=""><i class="icon icon-tack"></i></a>
                                </div>
                                <div class="article-controller-share-article">
                                    <a class="article-controller-share-button" title="">
                                        <span class="article-controller-share-button-open"><i class="icon icon-share"></i></span>
                                        <span class="article-controller-share-button-close"><i class="icon icon-close"></i></span>
                                    </a>
                                    <div class="article-controller-share-social">
                                        <a class="article-controller-share-facebook" title=""><i class="icon icon-facebook"></i></a>
                                        <a class="article-controller-share-twitter" title=""><i class="icon icon-twitter"></i></a>
                                        <a class="article-controller-share-linkedin" title=""><i class="icon icon-linkedin"></i></a>
                                        <a class="article-controller-share-google-plus" title=""><i class="icon icon-google-plus"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="article-category-related-container">
                            <ul>
                                <%_ for (var j = 0 ; j < data.articles.length; j++) { _%>
                                    <%_ var relatedArticleParent = data.articles[j]; _%>
                                    <%_ var relatedArticleParentId = relatedArticleParent.articleid; _%>
                                    <%_ for (var k = 0 ; k < eachArticle.relatedarticleid.length; k++) { _%>
                                        <%_ var relatedArticleRange = eachArticle.relatedarticleid[k]; _%>
                                        <%_ if ( relatedArticleParentId === relatedArticleRange ) { _%>
                                            <li id="article-<%- relatedArticleParent.articleid %>">
                                                <article>
                                                    <div class="article-category-container">
                                                        <a>
                                                            <%_ if ( relatedArticleParent.kickerimage350 ) { _%>
                                                            <div class="article-image <%- relatedArticleParent.imageorientation %> <%- relatedArticleParent.imagefullwidth %>">
                                                                <div class="new-article-image compat-object-fit" style="background-image: url('<%- relatedArticleParent.kickerimage350 %>');">
                                                                    <img src="<%- relatedArticleParent.kickerimage350 %>" alt="Image for <%- relatedArticleParent.kicker %> article" />
                                                                </div>
                                                            </div>
                                                            <%_ } _%>
                                                            <h2 class="article-kicker"><%- relatedArticleParent.kicker %></h2>
                                                            <h3 class="article-subhead"><%- relatedArticleParent.subhead %></h3>
                                                        </a>
                                                    </div>
                                                    <div class="article-additional-info">
                                                        <small class="article-authors-container">
                                                            By: 
                                                            <%_ if ( relatedArticleParent.reporter1 ) { _%>
                                                                <a href=""><span class="article-author"><%- relatedArticleParent.reporter1 %></span></a>
                                                            <%_ } _%>
                                                            <%_ if ( relatedArticleParent.reporter1 && relatedArticleParent.reporter2 ) { _%>
                                                                and <a href=""><span class="article-author"><%- relatedArticleParent.reporter2 %></span></a>
                                                            <%_ } _%>
                                                        </small>
                                                        <%_ if ( relatedArticleParent.allowcomments === 1 ) { _%>
                                                            <small class="article-comments-total-container">
                                                                <a href=""><i class="icon icon-comments"></i><span class="article-comments-total"><%- relatedArticleParent.articletotalcomments %></span><span class="article-comments-text">Comments</span></a>
                                                            </small>
                                                        <%_ } _%>
                                                    </div>
                                                    <div class="article-controller-container">
                                                        <div class="article-controller-pin-article">
                                                            <a title=""><i class="icon icon-tack"></i></a>
                                                        </div>
                                                        <div class="article-controller-share-article">
                                                            <a class="article-controller-share-button" title="">
                                                                <span class="article-controller-share-button-open"><i class="icon icon-share"></i></span>
                                                                <span class="article-controller-share-button-close"><i class="icon icon-close"></i></span>
                                                            </a>
                                                            <div class="article-controller-share-social">
                                                                <a class="article-controller-share-facebook" title=""><i class="icon icon-facebook"></i></a>
                                                                <a class="article-controller-share-twitter" title=""><i class="icon icon-twitter"></i></a>
                                                                <a class="article-controller-share-linkedin" title=""><i class="icon icon-linkedin"></i></a>
                                                                <a class="article-controller-share-google-plus" title=""><i class="icon icon-google-plus"></i></a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </article>
                                            </li>
                                        <%_ } _%>
                                    <%_ } _%>
                                <%_ } _%>
                            </ul>
                        </div>
                    </article>
                <%_ } _%>
            <%_ } _%>
        </div>
        <div name="trending" class="lead-story-left-column">
            <div class="content-area-category-title textb textup">
                Trending
            </div>
            <%_ for (var i = 0 ; i < data.articles.length; i++) { _%>
                <%_ eachHappeningNowTrendingArticle = data.articles[i]; _%>
                <%_ var thisHappeningNowTrendingArticle = eachHappeningNowTrendingArticle.happeningnowtrendingarticleid; _%>
                <%_ if ( thisHappeningNowTrendingArticle !== 0 ) { _%>
                    <article>
                        <div class="article-category-main-container">
                            <%_ if ( eachHappeningNowTrendingArticle.kickerimage350 ) { _%>
                            <div class="article-category-container">
                                <a>
                                    <div class="article-image <%- eachHappeningNowTrendingArticle.imageorientation %> <%- eachHappeningNowTrendingArticle.imagefullwidth %>">
                                        <div class="new-article-image compat-object-fit" style="background-image: url('<%- eachHappeningNowTrendingArticle.kickerimage350 %>');">
                                            <img src="<%- eachHappeningNowTrendingArticle.kickerimage350 %>" alt="Image for <%- eachHappeningNowTrendingArticle.kicker %> article" />
                                        </div>
                                    </div>
                                    <h1 class="article-headline"><%- eachHappeningNowTrendingArticle.headline %></h1>
                                    <div class="content-area-trending-incrementer"><%- eachHappeningNowTrendingArticle.happeningnowtrendingarticleid %></div>
                                </a>
                            </div>
                            <%_ } else { _%>
                            <div class="article-category-container">
                                <a>
                                    <h1 class="article-headline"><%- eachHappeningNowTrendingArticle.headline %></h1>
                                    <div class="article-nutshell"><%- eachHappeningNowTrendingArticle.nutshell %></div>
                                    <div class="content-area-trending-incrementer"><%- eachHappeningNowTrendingArticle.happeningnowtrendingarticleid %></div>
                                </a>
                            </div>
                            <div class="article-additional-info">
                                <small class="article-authors-container">
                                    By: 
                                    <%_ if ( eachHappeningNowTrendingArticle.reporter1 ) { _%>
                                        <a href=""><span class="article-author"><%- eachHappeningNowTrendingArticle.reporter1 %></span></a>
                                    <%_ } _%>
                                    <%_ if ( eachHappeningNowTrendingArticle.reporter1 && eachHappeningNowTrendingArticle.reporter2 ) { _%>
                                        and <a href=""><span class="article-author"><%- eachHappeningNowTrendingArticle.reporter2 %></span></a>
                                    <%_ } _%>
                                </small>
                                <%_ if ( eachHappeningNowTrendingArticle.allowcomments === 1 ) { _%>
                                <small class="article-comments-total-container">
                                    <a href=""><i class="icon icon-comments"></i><span class="article-comments-total"><%- eachHappeningNowTrendingArticle.articletotalcomments %></span><span class="article-comments-text">Comments</span></a>
                                </small>
                                <%_ } _%>
                            </div>
                            <%_ } _%>
                            <div class="article-controller-container">
                                <div class="article-controller-pin-article">
                                    <a title=""><i class="icon icon-tack"></i></a>
                                </div>
                                <div class="article-controller-share-article">
                                    <a class="article-controller-share-button" title="">
                                        <span class="article-controller-share-button-open"><i class="icon icon-share"></i></span>
                                        <span class="article-controller-share-button-close"><i class="icon icon-close"></i></span>
                                    </a>
                                    <div class="article-controller-share-social">
                                        <a class="article-controller-share-facebook" title=""><i class="icon icon-facebook"></i></a>
                                        <a class="article-controller-share-twitter" title=""><i class="icon icon-twitter"></i></a>
                                        <a class="article-controller-share-linkedin" title=""><i class="icon icon-linkedin"></i></a>
                                        <a class="article-controller-share-google-plus" title=""><i class="icon icon-google-plus"></i></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </article>
                <%_ } _%>
            <%_ } _%>
            <div class="content-area-trending-promo-area">
                <a>
                    <span class="content-area-trending-promo-title textb textup">Got a confidential news tip?</span>
                    <span class="content-area-trending-promo-content"><span class="textb">TPNS</span><span class="texti">two</span> offers several ways to get in touch with and provide materials to our journalists, find out more here.</span>
                </a>
            </div>
        </div>
        <div class="content-area-latest-articles">
            <div class="content-area-latest-articles-top-promo-area">
                <div class="content-area-latest-articles-top-promo-area-split">
                    <div class="content-area-category-title textb textup">
                        Podcasts
                    </div>
                    <%_ var podcastArticleCounter = 0; _%>
                    <%_ for (var i = 0 ; i < data.podcasts.length; i++) { _%>
                        <%_ eachPodcastArticle = data.podcasts[i]; _%>
                        <%_ var thisPodcastArticle = eachPodcastArticle.categoryslug; _%>
                        <%_ if ( thisPodcastArticle === "the-daily" ) { _%>
                            <%_ if (podcastArticleCounter < 1) { _%>
                                <podcast id="article-<%- eachPodcastArticle.articleid %>">
                                    <div class="article-category-subcategory-container textup">
                                        <a href="/<%- eachPodcastArticle.categoryslug %>" title=""><%- eachPodcastArticle.category %></a>
                                    </div>
                                    <div class="article-category-main-container">
                                        <div class="article-category-container">
                                            <a>
                                                <h1 class="article-headline"><%- eachPodcastArticle.headline %></h1>
                                                <div class="article-nutshell"><%- eachPodcastArticle.nutshell %></div>
                                            </a>
                                        </div>
                                        <div class="audio-wrapper">
                                            <audio preload="none" controls>
                                                <source src="<%- eachPodcastArticle.podcastmp3 %>" type="audio/mp3" />
                                            </audio>
                                        </div>
                                        <div class="article-additional-info">
                                            <small class="article-category-subscribe-item">
                                                Subscribe for <span class="texteb">FREE</span> below:
                                            </small>
                                            <small class="article-category-subscribe-item">
                                                <a href="<%- eachPodcastArticle.applepodcast %>" target="_blank">Apple Podcasts</a>
                                            </small>
                                            <small class="article-category-subscribe-item">
                                                <a href="<%- eachPodcastArticle.googleplaymusic %>" target="_blank">Google Play Music</a>
                                            </small>
                                            <small class="article-category-subscribe-item">
                                                <a href="<%- eachPodcastArticle.radiopublic %>" target="_blank">Radio Public</a>
                                            </small>
                                        </div>
                                    </div>
                                </podcast>
                                <%_ podcastArticleCounter ++; _%>
                            <%_ } _%>
                        <%_ } _%>
                    <%_ } _%>
                </div>
                <div class="content-area-latest-articles-top-promo-area-split">
                    <div class="content-area-category-title textr textb textup">
                        Topics
                    </div>
                    <div class="article-category-subcategory-container textup textc">
                        <a href="/tpnstwo-magazine" title="">TPNStwo Magazine</a>
                    </div>
                    <div class="content-area-latest-articles-top-promo-inner-area">
                        <%_ var insiderArticleCounter = 0; _%>
